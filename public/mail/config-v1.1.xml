<clientConfig version="1.1">
  <emailProvider id="gui.dev.br">
    <domain>gui.dev.br</domain>
    <displayName>gui.dev.br</displayName>
    <displayShortName>gui.dev.br</displayShortName>
    <incomingServer type="imap">
      <hostname>selfhost.gui.dev.br</hostname>
      <port>143</port>
      <socketType>STARTTLS</socketType>
      <authentication>password-cleartext</authentication>
      <username>%EMAILADDRESS%</username>
    </incomingServer>
	<outgoingServer type="smtp">
      <hostname>selfhost.gui.dev.br</hostname>
      <port>587</port>
      <socketType>STARTTLS</socketType>
      <authentication>password-cleartext</authentication>
      <username>%EMAILADDRESS%</username>
    </outgoingServer>
  </emailProvider>
</clientConfig>
